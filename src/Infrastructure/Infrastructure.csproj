    <UserSecretsId>9c947c10-2373-4590-92a9-e5fe6b759c69</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.8" />
